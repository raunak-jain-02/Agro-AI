{
  "rewrites": [
    {
      "source": "/api/gov/:path*",
      "destination": "https://api.data.gov.in/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/send-weather-alerts.ts": {
      "maxDuration": 300
    },
    "api/send-price-alerts.ts": {
      "maxDuration": 300
    },
    "api/send-welcome-email.ts": {
      "maxDuration": 60
    }
  }
}